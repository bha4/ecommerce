const productModel =require("../models/productModel")


exports.getProducts= async (req, res, next)=>{
  const products=await productModel.find({})
  res.json({
    sucess:true,
    products
})
}

exports.getSingleProduct=(req, res, next)=> {
  res.json({
    sucess:true,
    message:"Get single product working!",
  });
}